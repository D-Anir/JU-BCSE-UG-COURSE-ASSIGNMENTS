			;F4H
LDA 2050H	;11110100 -> A
MOV B,A		;A -> B
ANI 0FH 	;A & 0FH
			;00001111
RLC
RLC
RLC
RLC

MOV C,A 	;00000100(A) -> C

MOV A,B 	;B -> A
ANI F0H		;A & F0H
			;11110000
RLC
RLC
RLC
RLC

MOV D,A		;11110000(A) -> D

			;E1H
LDA 2051H	;11100001 -> A
MOV B,A		;A -> B
ANI 0FH		;A & 0FH
			;00001111
RLC
RLC
RLC
RLC

MOV E,A		;00000100(A) -> E

MOV A,B		;B -> A
ANI F0H		;A & F0H
			;11110000
RLC
RLC
RLC
RLC

MOV B,A		;11110000(A) -> H

MOV A,C
ADD D
STA 2052H

MOV A,B
ADD E
STA 2053H

HLT